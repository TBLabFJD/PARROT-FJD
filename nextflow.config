process {
	cpus = 2
	memory = 10.GB

	withLabel: mediumcpu { cpus = params.mediumcpu } // HAPLOTYPECALLER, SELECT_SNV, SELECT_INDEL, SELECT_MIX, FILTRATION_SNV, FILTRATION_INDEL, FILTRATION_MIX, MERGE_VCF, GVCF_HAPLOTYPECALLER
	withLabel: mediummem { memory = "${params.mediummem} GB" } // HAPLOTYPECALLER, SELECT_SNV, SELECT_INDEL, SELECT_MIX, FILTRATION_SNV, FILTRATION_INDEL, FILTRATION_MIX, MERGE_VCF, GVCF_HAPLOTYPECALLER
	
	withLabel: highcpu { cpus = params.highcpu } // BWA, MARKDUPLICATESSPARK, VEP, DEEPVARIANT, ANNOTSV, MANTA, ANNOTSV_VCF
	withLabel: highmem { memory = "${params.highmem} GB" } // BWA, MARKDUPLICATESSPARK, VEP, DEEPVARIANT, ANNOTSV, MANTA, ANNOTSV_VCF, PVM, QC_SUMMARY

