
manifest {
	 description = 'FJD-pipeline for germline Variant Calling Analysis based on GATK best practices.'
	 nextflowVersion = '>= 21.10.6'
}

// cleanup = true


params {

	// Input directory or BaseSpace project name (if --analysis D is specified) 
	input     = []

	// Output directory to copy all results. If it does not exists it is created.
	output    = []
	
	// Name of the analysis, if not provided the launching time is taken as the runname
	runname   = []

	// Analysis to perform: 
			// D (Download), 
			// M (Mapping), 
			// Q (quality), 
			// S (SNV individual), 
			// G (SNV GVCF), 
			// A (SNV annotation), 
			// C (CNV calling and annotation)
	// "--analysis MSA" will perform mapping, SNV calling and annotation
	analysis = [] 
	
	vc_tools="gatk"
	// tools_availables:
	
		//Haplotypecaller -> gatk
		//DRAGEN -> dragen
		//DeepVariant -> deepvariant
		//all
		// Ej. --vc_tools gatk,dragen

	// Type of capture: P (Panel), E (Whole Exome), G (Whole Genome)
	// The only difference right now is the CNV calling of WGS vs Panel and WES
	capture  = "P" 

	// parallel processes
	parallel_mapping=false
	parallel_calling=false

	//mosdepth_bed
	mosdepth_bed=false

	//make cram
	keep_cram=false

	// Not implemented: stop the execution if a sample is missing
	// errormissingsample = true

	// File containing the list with the samples to analyce (in a single column)
	samples  = []
	
	// File containing the list of genes to report (in a single column).
	// If glowgenes is also specified, then it does not acts as a filter, 
	// if not as part of the priorization giving the value "0" to the specified genes
	genelist = []
	
	// File containing the output of GLOWgenes
	glowgenes  = []

	// Four column bed file containing the captured regions. 
	// Required when analycing CNVs and/or when "--intervals" is indicated
	bed       = []

	// Ped file containing the family information
	ped       = []

	// Call SNVs/INDELs in the specified regions
	intervals  = false

	// Number of bases to spand the interval regions for SNV/INDEL calling
	padding    = 500
	
	// Bin length when fractioning bed file for CNV calling
	window     = []

	// minimum target size for CNV calling
	min_target = 20

	// Call CNVs in chromosome X (not tested)
	cnv_chrx = false

	// Ignore dopplicates in the mapping workforw (it avoids running the markduplicates process)
	ignoreduplicates = [] // In case of amplicon-based library preparation it is better to ignore duplicates
	
	// Not implemented: Read depth that 90% ofregions must have to pass QC filter
	// qcnvthreshold  = 20 

	// Not implemented: Read depth that 90% ofregions must have to pass QC filter
	// qsnvthreshold = 10

	
	// Not implemented: idicate the pathology to retrieve annotation of variant frequency in cases and pseudocontrols
	// Disease group name: resp, digest, skin, musc, gu, pregnancy, perinatal, cong_mal, clinic, infectious, other, neoplasm, blood, endoc, mental, CNS, eye, ear, circ, healthy.
	// pathology = "healthy" 	

	// Minor Allele Frequenct (MAF) filter. It filters out variants fith alleles frequencies above this threshold
	maf = "0.1"
	
	// Not implemented: avoid copying the bam files 
	// removebam  = []


	// Not tested. BaseSpace user different from the one in the configuration file created during authentication
	baseuser  = []

	// Slurm queue
	queue = "bioinfo" // bioinfo, fastbioinfo

	// Resources for medium demanding proccess
	mediumcpu = "4"
	mediummem = "20"

}











